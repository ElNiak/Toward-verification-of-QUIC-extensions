client [options] URL [URL...]
	[-i interface]	interface to run over; default lo
	[-s cache]	TLS 0-RTT state cache; default /tmp/quant-session
	[-l log]	log file for TLS keys; default /tmp/quant-tlslog
	[-t timeout]	idle timeout in seconds; default 10
	[-c]		verify TLS certificates; default false
	[-u]		update TLS keys; default false
	[-3]		send a static H3 request; default false
	[-z]		use zero-length source connection IDs; default false
	[-w]		write retrieved objects to disk; default false
	[-r reps]	repetitions for all URLs; default 1
	[-b bufs]	number of network buffers to allocate; default 100000
	[-n]		simulate a NAT rebinding for each URL; default false
	[-v verbosity]	verbosity level (0-5, default 5)
